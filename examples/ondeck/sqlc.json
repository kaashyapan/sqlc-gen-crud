{
  "version": "2",
  "plugins": [
    {
      "name": "gen-crud",
      "process": {
        "cmd": "/home/ubuntu/bin/sqlc-gen-crud"
      }
    }
  ],
  "sql": [
    {
      "schema": "postgresql/schema",
      "queries": "dummy.sql",
      "engine": "postgresql",
      "codegen": [
        {
          "out": "postgresql",
          "plugin": "gen-crud",
          "options": {}
        }
      ]
    },
    {
      "schema": "mysql/schema",
      "queries": "dummy.sql",
      "engine": "mysql",
      "codegen": [
        {
          "out": "mysql",
          "plugin": "gen-crud",
          "options": {}
        }
      ]
    },
    {
      "schema": "sqlite/schema",
      "queries": "dummy.sql",
      "engine": "sqlite",
      "codegen": [
        {
          "out": "sqlite",
          "plugin": "gen-crud",
          "options": {}
        }
      ]
    }
  ]
}